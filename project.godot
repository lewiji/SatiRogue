[laigter_config]

GENERATE_SPECULAR=false
GENERATE_OCCLUSION=false
GENERATE_PARALLAX=false
HIDE_LAIGTER_GUI=false

[logging]

file_logging/enable_file_logging.pc=false

[memory]

limits/message_queue/max_size_kb=8192

[physics]

common/physics_jitter_fix=0.0
common/physics_interpolation=true
common/enable_object_picking=false

[rendering]

2d/snapping/use_gpu_pixel_snap=true
threads/thread_model=2
quality/shadows/filter_mode=0
quality/filters/anisotropic_filter_level=16
quality/spatial_partitioning/render_tree_balance=1.0
2d/options/ninepatch_mode=0
gles3/shaders/shader_cache_size_mb=1024
environment/default_clear_color=Color( 0, 0, 0, 1 )
