} ]
_global_script_class_icons={
"InspectedObjectGetter": "",
"LTConfig": "",
"LaigterCli": "",
"LaigterCliResult": ""
}

[application]

config/name="RoguelikeMono"
run/main_scene="res://Main.tscn"
run/delta_sync_after_draw=true
boot_splash/show_image=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[audio]

output_latency=64

[autoload]

TextTransitionSettings="*res://addons/teeb.text_transitions/TextTransitionSettings.gd"
Logger="*res://src/Debug/Logger.cs"
GameScheduler="*res://src/Tools/GameScheduler.cs"

[debug]

settings/physics_interpolation/enable_warnings=false

[display]

window/size/width=900
window/size/height=900
window/size/always_on_top=true
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor]

convert_text_resources_to_binary_on_export=true

[editor_plugins]

enabled=PoolStringArray( "res://addons/float_step_adjuster/plugin.cfg", "res://addons/godot-better-animated-texture/plugin.cfg", "res://addons/godot-plugin-refresher/plugin.cfg", "res://addons/kanban_tasks/plugin.cfg", "res://addons/laigter_tools/plugin.cfg", "res://addons/zoomy/plugin.cfg" )

